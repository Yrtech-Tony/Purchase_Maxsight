@using Purchase.Web.Common
@model Purchase.DAL.Requirement_Shenfangjirijiliuzhi_RequirementDtl6

<form id="Shenfangjirijiliuzhi-dtl6-form">
    @Html.Hidden("SeqNO", 0)
    <div class="form-horizontal">
        <div class="form-group">
            <label class="control-label col-md-2" for="">录音类型</label>
            <div class="col-md-8">
                @Html.DropDownListFor(m => m.luyin, SelectListTool.HiddenCodeList("需求书-深访及日记留置-资料提交-录音类型"),
                           htmlAttributes: new { @class = "form-control", multiple = "" })
            </div>
        </div>
        <div class="form-group">
            <label class="control-label col-md-2" for="">合格录音比例</label>
            <div class="col-md-8">
                @Html.DropDownListFor(m => m.hegeluyinbili, SelectListTool.HiddenCodeList("需求书-深访及日记留置-资料提交-合格录音比例"),
                           htmlAttributes: new { @class = "form-control", multiple = "" })
            </div>
        </div>
        <div class="form-group">
            <label class="control-label col-md-2" for="">证照类型</label>
            <div class="col-md-8">
                @Html.DropDownListFor(m => m.zhengzhaoleixing, SelectListTool.HiddenCodeList("需求书-深访及日记留置-资料提交-证照类型"),
                           htmlAttributes: new { @class = "form-control", multiple = "" })
            </div>
        </div>

        <div class="form-group">
            <label class="control-label col-md-2" for="">合格证照比例</label>
            <div class="col-md-8">
                @Html.DropDownListFor(m => m.hegezhengzhaobili, SelectListTool.HiddenCodeList("需求书-深访及日记留置-资料提交-合格证照比例"),
                           htmlAttributes: new { @class = "form-control", multiple = "" })
            </div>
        </div>
        <div class="form-group">
            <label class="control-label col-md-2" for="">现场到车率</label>
            <div class="col-md-8">
                @Html.DropDownListFor(m => m.xianchangdaochelv, SelectListTool.HiddenCodeList("需求书-深访及日记留置-资料提交-现场到车率"),
                           htmlAttributes: new { @class = "form-control", multiple = "" })
            </div>
        </div>
        <div class="form-group">
            <label class="control-label col-md-2" for="RemarkDtl6">其他说明</label>
            <div class="col-md-8">
                @Html.TextAreaFor(m => m.qitashuoming6, htmlAttributes: new { @class = "form-control", multiple="" })
            </div>
        </div>
        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input id="saveBtn" type="button" value="保存" class="btn btn-primary btn-check" data-loading-text="提交中..." style="width:100px;height:35px" />
            </div>
        </div>
    </div>
</form>
<script>
    $(function () {
        
        $("#Shenfangjirijiliuzhi-dtl6-form #saveBtn").click(function () {
            if (!CommitCheck()) {
                alert("正在审核中或者非本人添加的项目");
                return false;
            }
            $(this).button("loading");
            var params = $("#Shenfangjirijiliuzhi-dtl6-form").serializeJson('&');
            params = $.extend(params, getMstObject());

            $.post("/DemandBook/SaveShenfangjirijiliuzhiDtl6", params, function (data) {
                $("#RequirementId").val(data);
                loadShenfangjirijiliuzhiDtl6();
                closeModal();
            });
        })
    })
</script>