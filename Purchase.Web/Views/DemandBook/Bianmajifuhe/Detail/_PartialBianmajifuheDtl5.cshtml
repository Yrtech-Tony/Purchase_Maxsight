<script>
    $(function () {
       
        $("#Bianmajifuhe-dtl5-form #saveBtn").click(function () {
            if (!CommitCheck()) {
                alert("正在审核中或者非本人添加的项目");
                return false;
            }
            $(this).button("loading")
            var params = $("#Bianmajifuhe-dtl5-form").serializeJson('&');
            params = $.extend(params, getMstObject());

            $.post("/DemandBook/SaveBianmajifuheDtl5", params, function (data) {
                $("#RequirementId").val(data);
                loadBianmajifuheDtl5();
                closeModal();
            });
        })
    })
</script>