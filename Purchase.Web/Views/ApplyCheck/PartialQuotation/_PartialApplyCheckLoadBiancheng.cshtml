@using Purchase.Service.DTO
@model IEnumerable<Quotation_BianChengDto>
<table id="quotation-table" class="table table-bordered table-condensed table-hover">
    <thead>
        <tr>
            <th>
                供应商名称
            </th>
            <th>
                执行省份
            </th>
            <th>
                执行城市
            </th>
            <th>
                执行方式
            </th>
            <th>
                工作分类
            </th>
            <th>
                工作内容
            </th>
            <th>
                类型/品牌
            </th>
            <th>
                规格/要求
            </th>
            <th>
                质量标准
            </th>
            <th>
                核算单位
            </th>
            <th>
                单价
            </th>
            <th>
                数量
            </th>
            <th>
                小计
            </th>
            <th>
                备注
            </th>
        </tr>
    </thead>
    <tbody>
        @if (Model != null)
        {
            foreach (var item in Model)
            {
                double danjia = item.danjia.HasValue ? (double)item.danjia.Value : 0;
                double shuliang = Purchase.Common.Utils.String2Double(item.shuliang);
                var xiaoji = danjia * shuliang;
                <tr>
                    <td>@item.SupplierName</td>
                    <td>@item.zhixingshengfen</td>
                    <td>@item.zhixingchengshi</td>
                    <td>@item.zhixingfangshi</td>
                    <td>@item.gongzuofenlei</td>
                    <td>@item.gongzuoneirong</td>
                    <td>@item.leixingpinpai</td>
                    <td>@item.guigeyaoqiu</td>
                    <td>@item.zhiliangbiaozhun</td>
                    <td>@item.hesuandanwei</td>
                    <td>@Math.Round(danjia, 2)</td>
                    <td>@item.shuliang</td>
                    <td>@Math.Round(xiaoji, 2)</td>
                    <td style="text-align:left">@item.beizhu</td>
                </tr>
            }
        }
    </tbody>
</table>
<script>
    $(function () {
    })

</script>