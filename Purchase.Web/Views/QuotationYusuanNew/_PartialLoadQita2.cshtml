<table id="quotation-table" class="table table-bordered table-condensed table-hover">
    <thead>
        <tr>
            <th>
                供应商名称
            </th>
            <th>
                服务行业
            </th>
            <th>
                采购分类
            </th>
            <th>
                采购方式
            </th>
            <th>
                提供服务
            </th>
            <th>
                费用构成
            </th>
            <th>
                品名
            </th>
            <th>
                规格
            </th>
            <th>
                型号
            </th>
            <th>
                参考价格
            </th>
            <th>
                预算单价
            </th>
            <th>
                预算数量
            </th>
            <th>
                预算小计
            </th>
            <th>
                单价
